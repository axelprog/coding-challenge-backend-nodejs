{
  "name": "q",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "prestart": "npm run docs",
    "start": "node ./bin/start",
    "test": "mocha --recursive ./test/**/*.test.js --require babel-core/register --exit",
    "test:coverage": "nyc report npm test",
    "docs": "apidoc -i controllers/ -o apidoc/",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern node_modules/* --ignore-pattern apidoc/*",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "apidoc": "^0.17.7",
    "babel-core": "^6.25.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^3.0.0",
    "express": "^4.15.4",
    "express-interceptor": "^1.2.0",
    "express-validation": "^1.0.2",
    "helmet": "^3.15.0",
    "http-status": "^1.3.1",
    "joi": "^14.3.0",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.4",
    "node-sanitizer": "^1.0.5",
    "response-time": "^2.3.2",
    "sequelize": "^4.42.0",
    "serve-favicon": "^2.4.3",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.2.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  }
}
